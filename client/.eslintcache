e = await server.get(url);\r\n  return Object.keys(response.pricing).map((pricing) => {\r\n    return {\r\n      value: pricing,\r\n      label: pricing.charAt(0).toUpperCase() + pricing.slice(1),\r\n    };\r\n  });\r\n};\r\n\r\nconst useDynamicSelect = (\r\n  initialVal,\r\n  url = undefined,\r\n  defaultOption = undefined\r\n) => {\r\n  const [data, setData] = useState(initialVal);\r\n  useEffect(() => {\r\n    if (initialVal.length > 1) {\r\n      if (defaultOption) {\r\n        setData([defaultOption, ...initialVal]);\r\n        return;\r\n      }\r\n      setData(initialVal);\r\n      return;\r\n    }\r\n    fetch(url).then((response) => {\r\n      if (defaultOption) {\r\n        setData([defaultOption, ...response]);\r\n        return;\r\n      }\r\n\r\n      setData(response);\r\n    });\r\n  }, []);\r\n\r\n  return data;\r\n};\r\n\r\nexport default useDynamicSelect;\r\n","C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\context\\ModalContext.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\user-table\\row\\TableRow.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\user-table\\actions\\Actions.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\user-table\\row\\EditableRow.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\switch\\Switch.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\shared\\utils\\constants.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\shared\\utils\\data.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\shared\\components\\Modal\\Modal.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\shared\\components\\Modal\\Styles.js",["312"],"import styled, { css } from 'styled-components';\r\n\r\nimport { COLORS } from '../../utils/colors';\r\nimport