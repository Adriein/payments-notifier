Submited({\r\n        status: SUCCESS,\r\n        msg: 'El excel se ha subido correctamente',\r\n      });\r\n    } catch (error) {\r\n      setSubmited({ status: ERROR, msg: error.msg });\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"uploader\">\r\n      {submited.status === DEFAULT && (\r\n        <div>\r\n          <input id=\"uploader\" type=\"file\" hidden=\"hidden\" />\r\n          <button onClick={handleUpload} className=\"button button--primary\">\r\n            Seleccionar\r\n          </button>\r\n          <span className=\"span\">Ningún excel seleccionado...</span>\r\n        </div>\r\n      )}\r\n      {submited.status === SUCCESS && <p>{submited.msg} ✅</p>}\r\n      {submited.status === ERROR && <p>{submited.msg} ❌</p>}\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\copyright\\Copyright.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\api\\server.js",[],["40","41"],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\constants.js",[],{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","severity":1,"message":"47","line":16,"column":6,"nodeType":"48","endLine":16,"endColumn":12,"suggestions":"49"},{"ruleId":"42","replacedBy":"50"},{"ruleId":"44","replacedBy":"51"},"no-native-reassign",["52"],"no-negated-in-lhs",["53"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'upload'. Either include it or remove the dependency array.","ArrayExpression",["54"],["52"],["53"],"no-global-assign","no-unsafe-negation",{"desc":"55","fix":"56"},"Update the dependencies array to be: [file, upload]",{"range":"57","text":"58"},[454,460],"[file, upload]"]