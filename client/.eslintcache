        <input id=\"uploader\" type=\"file\" hidden=\"hidden\" />\r\n            <button onClick={handleUpload} className=\"button button--primary\">\r\n              Seleccionar\r\n            </button>\r\n            <span className=\"span\">Ningún excel seleccionado...</span>\r\n          </div>\r\n        )}\r\n        {submited.status === SUCCESS && !isLoading && <p>{submited.msg} ✅</p>}\r\n        {submited.status === ERROR && !isLoading && <p>{submited.msg} ❌</p>}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\copyright\\Copyright.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\api\\server.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\constants.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\ProtectedRoute.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\context\\AuthContext.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\context\\UsersContext.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\pages\\HomePage.js",["134","135"],"import React, { useContext } from 'react';\r\nimport './HomePage.scss';\r\nimport { Context as UsersContext } from '../context/UsersContext';\r\nimport Copyright from '../components/copyright/Copyright';\r\nimport NavBar from '../components/nav-bar/NavBar';\r\nimport Uploader from '../components/uploader/Uploader';\r\nimport UserTable from '../components/user-table/UserTable';\r\nimport UserForm from '../components/user-form/UserForm';\r\n\r\nexport default function HomePage() {\r\n  const { state } = useContext(UsersContext);\r\n  return (\r\n    <div className=\"page__container--main\">\r\n      <NavBar />\r\n      <div className=\"wrapper\">\r\n        {/* <Uploader /> */}\r\n        {state.createUser.status === 'create' ? <UserForm /> : <UserTable />}\r\n 