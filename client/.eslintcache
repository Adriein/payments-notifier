sponse.data.errors;\r\n      setSubmited({\r\n        status: ERROR,\r\n        msg: `Field: ${errormsg.field} Msg: ${errormsg.message}`,\r\n      });\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div>\r\n      {/* <div class=\"drop-zone\">\r\n        <span class=\"drop-zone__prompt\">Arrastra un archivo o pincha para subir</span>\r\n        <input type=\"file\" name=\"myFile\" class=\"drop-zone__input\" />\r\n        <div className=\"drop-zone__thumb\">archivo defaulters.xlsx</div>\r\n      </div> */}\r\n      <div className=\"uploader\">\r\n        {submited.status === DEFAULT && !isLoading && (\r\n          <div>\r\n            <input id=\"uploader\" type=\"file\" hidden=\"hidden\" />\r\n            <button onClick={handleUpload} className=\"button button--primary\">\r\n              Seleccionar\r\n            </button>\r\n            <span className=\"span\">Ningún excel seleccionado...</span>\r\n          </div>\r\n        )}\r\n        {submited.status === SUCCESS && !isLoading && <p>{submited.msg} ✅</p>}\r\n        {submited.status === ERROR && !isLoading && <p>{submited.msg} ❌</p>}\r\n        {isLoading && <Loader />}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\copyright\\Copyright.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\api\\server.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\constants.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\Loader\\Loader.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\Loader\\Triangle\\Triangle.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\ProtectedRoute.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\context\\AuthContext.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\context\\UsersCon