nents\\login\\Login.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\uploader\\Uploader.js",["79"],"import React, { useEffect, useState } from 'react';\r\nimport server from '../../api/server';\r\nimport { DEFAULT, ERROR, SUCCESS } from '../../constants';\r\nimport './Uploader.css';\r\nimport Loader from '../Loader/Loader';\r\n\r\nexport default function Uploader() {\r\n  const [submited, setSubmited] = useState({ status: DEFAULT, msg: '' });\r\n  const [file, setFile] = useState(undefined);\r\n  const [isLoading] = useState(false);\r\n\r\n  useEffect(() => {\r\n    (async () => {\r\n      if (file) {\r\n        await upload();\r\n      }\r\n    })();\r\n  }, [file]);\r\n\r\n  useEffect(() => {\r\n    let timer;\r\n    if (submited.status !== DEFAULT) {\r\n      timer = setTimeout(() => {\r\n        setSubmited({ status: DEFAULT, msg: '' });\r\n      }, 6000);\r\n    }\r\n    return () => {\r\n      if (timer) clearTimeout(timer);\r\n    };\r\n  }, [submited]);\r\n\r\n  const handleUpload = () => {\r\n    document.getElementById('uploader').click();\r\n    document.getElementById('uploader').onchange = (event) => {\r\n      setFile(event.target.files[0]);\r\n    };\r\n  };\r\n\r\n  const upload = async () => {\r\n    const data = new FormData();\r\n    data.append('defaulters', file);\r\n    try {\r\n      await server.post('upload', data);\r\n      setSubmited({\r\n        status: SUCCESS,\r\n        msg: 'El excel se ha subido correctamente',\r\n      });\r\n    } catch (error) {\r\n      const [errormsg] = error.response.data.errors;\r\n      setSubmited({\r\n        status: ERROR,\r\n        msg: `Field: ${errormsg.field} Msg: ${errormsg.message}`,\r\n      });\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div>\r\n      {/* <div class=\"drop-zone\">\r\n        <span class=\"drop-zone__prompt\">Arrastra un archivo o pincha para subir</span>\r\n        <input type=\"file\" name=\"myFile\" class=\"drop-zone__input\" />\r\n        <div className=\"drop-zone__thumb\">archivo defaulters.xlsx</div>\r\n      </div> */}\r\n      <div className=\"uploader\">\r\n        {submited.status === DEFAULT && !isLoading && (\r\n          <div>\r\n            <input id=\"uploader\" type=\"file\" hidden=\"hidden\" />\r\n            <button onClick={handleUpload} className=\"button button--primary\">\r\n              Seleccionar\r\n            </button>\r\n            <span className=\"span\">Ningún excel seleccionado...</span>\r\n          </div>\r\n        )}\r\n        {submited.status === SUCCESS && !isLoading && <p>{submited.msg} ✅</p>}\r\n        {submited.status === ERROR && !isLoading && <p>{submited.msg} ❌</p>}\r\n        {isLoading && <Loader />}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\copyright\\Copyright.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\api\\server.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\constants.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\Loader\\Loader.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\Loader\\Triangle\\Triangle.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\ProtectedRoute.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\context\\AuthContext.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\context\\UsersContext.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\pages\\HomePage.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\context\\createDataContext.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\hooks\\useInputState.js",[],"C:\\Users\\siriu\\Desktop\\CLARET-DEV\\payments-notifier\\client\\src\\components\\UserTable\\UserTab